{
  "name": "syllabus-scrape",
  "type": "module",
  "scripts": {
    "start": "tsx ./src/main.ts",
    "build": "tsc && tsc-alias -f",
    "clean": "rm -rf ./dist",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write",
    "lint": "biome lint .",
    "check": "biome check --write .",
    "test": "jest"
  },
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "puppeteer": "^24.16.2"
  }
}
